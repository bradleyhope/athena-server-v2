# Athena Server v2 - Environment Variables
# Copy this file to .env and fill in the values

# Server
PORT=3001
ATHENA_API_KEY=your_api_key_here

# Database (Neon PostgreSQL)
DATABASE_URL=postgresql://user:password@host/database?sslmode=require

# Manus API
MANUS_API_KEY=sk-your_manus_api_key
MANUS_API_BASE=https://api.manus.im/v1

# AI Models
OPENAI_API_KEY=sk-your_openai_key
ANTHROPIC_API_KEY=sk-ant-your_anthropic_key

# Google OAuth
GOOGLE_CLIENT_ID=your_client_id.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=GOCSPX-your_secret
GOOGLE_REFRESH_TOKEN=1//your_refresh_token

# Notion
NOTION_API_KEY=ntn_your_notion_key

# Budget
MONTHLY_AI_BUDGET=500
